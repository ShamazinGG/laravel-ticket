{
    "name": "shamazingg/laravel-ticket",
    "description": "Laravel Ticket System, to help you manage your tickets eaisly",
    "keywords": [
        "Coderflex",
        "laravel",
        "laravel-ticket"
    ],
    "homepage": "https://github.com/ShamazinGG/laravel-ticket",
    "license": "MIT",
    "authors": [
        {
            "name": "Oussama Sid",
            "email": "oussama@coderflex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Coderflex\\LaravelTicket\\": "src",
            "Coderflex\\LaravelTicket\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coderflex\\LaravelTicket\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LaravelTicket": "Coderflex\\LaravelTicket\\Facades\\LaravelTicket"
            },
            "providers": [
                "Coderflex\\LaravelTicket\\TicketServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
